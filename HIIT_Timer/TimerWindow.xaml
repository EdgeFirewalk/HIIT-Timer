<Window x:Name="HIIT_Timer" x:Class="HIIT_Timer.TimerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HIIT_Timer"
        mc:Ignorable="d"
        Title="HIIT Timer" Height="450" Width="800" Closed="Window_Closed" Loaded="Window_Loaded" Background="#FF2E4156" ResizeMode="NoResize" KeyDown="HIIT_Timer_KeyDown" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="timeLabel" Content="0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="390" Width="794" Background="{x:Null}" FontSize="300" Foreground="White" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="0,-46,0,0"/>
        <Label x:Name="stageLabel" Content="Working Out!" HorizontalAlignment="Left" Margin="506,335,0,0" VerticalAlignment="Top" Height="86" Width="288" Background="{x:Null}" FontSize="45" Foreground="White" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Label x:Name="exerciseLabel" Content="" HorizontalAlignment="Left" Margin="0,335,0,0" VerticalAlignment="Top" Height="86" Width="288" Background="{x:Null}" FontSize="45" Foreground="White" VerticalContentAlignment="Center"/>
    </Grid>
</Window>
